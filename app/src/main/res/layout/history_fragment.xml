    <data>

        <variable
            name="viewModel"
            type="com.hmarka.kloop1996.transferelegance.viewmodel.HistoryViewModel" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
